    <owl:AnnotationProperty rdf:about="http://owl.essaid.com/owlcl/owlcl_module_unclassified_import_iri">
        <rdfs:comment>See the classified version of this annotation.</rdfs:comment>
    </owl:AnnotationProperty>
    


